{
  "name": "ets-skel-webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf .next pages ssr components hooks layouts styles utils types tsconfig.build.tsbuildinfo tsconfig.tsbuildinfo",
    "dev": "nodemon -x 'yarn next:dev'",
    "dev:infinite": "nodemon -x '/bin/sh -c \"yarn next:dev || touch next.config.js\"'",
    "next:dev": "NODE_OPTIONS='--max-old-space-size=6144 --inspect=127.0.0.1:9230' next dev",
    "dev:debug": "NODE_OPTIONS='--max-old-space-size=6144 --inspect=127.0.0.1:9230' next dev",
    "build:dev": "tsc --build",
    "watch": "yarn build --watch",
    "compile": "tsc",
    "build": "tsc --build tsconfig.build.json",
    "build-next": "next build",
    "start": "next start -p $PORT",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint .",
    "link-libs": "yarn link-libs-int && rm -rf .next",
    "link-libs-int": "yarn link @ets-skel/types @ets-skel/client @effect-ts/core @effect-ts/system @effect-ts/monocle @effect-ts/morphic @effect-ts/schema @effect-ts/tracing-plugin @effect-ts-app/core @effect-ts-app/fluent",
    "unlink-libs": "yarn unlink-libs-int && rm -rf .next",
    "unlink-libs-int": "yarn unlink @ets-skel/types @ets-skel/client @effect-ts/core @effect-ts/system @effect-ts/monocle @effect-ts/morphic @effect-ts/schema @effect-ts/tracing-plugin @effect-ts-app/core @effect-ts-app/fluent && yarn install --frozen-lockfile --force",
    "prettier": "prettier \"**/*.ts\"",
    "autofix": "yarn lint --fix && yarn prettier --write",
    "ncu": "ncu -x swr,intro.js,@date-io/date-fns,@material-ui/core,@material-ui/lab,ts-patch",
    "test": "jest",
    "testsuite": "yarn lint && yarn test --passWithNoTests",
    "up": "yarn update && yarn exec-update && yarn link-libs",
    "update": "yarn ncu -u",
    "update-libs": "yarn ncu -u --filter \"@ets-skel/*\" --pre 1",
    "exec-update": "yarn && yarn upgrade",
    "postinstall": "ts-patch install && patch-package"
  },
  "resolutions": {
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^1.5.0",
    "@azure/storage-blob": "^12.8.0",
    "@date-io/date-fns": "^1.3.13",
    "@effect-ts-app/core": "^0.39.2",
    "@effect-ts-app/fluent": "^0.39.1",
    "@effect-ts-app/react": "^0.39.0",
    "@effect-ts/core": "^0.44.4",
    "@effect-ts/fluent": "^0.3.12",
    "@effect-ts/monocle": "^0.35.0",
    "@effect-ts/morphic": "^0.37.1",
    "@effect-ts/schema": "^0.10.0",
    "@material-ui/core": "4.11.4",
    "@material-ui/data-grid": "4.0.0-alpha.37",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "4.0.0-alpha.58",
    "@material-ui/pickers": "^3.3.10",
    "@next/bundle-analyzer": "^11.1.2",
    "@sentry/browser": "^6.12.0",
    "@sentry/nextjs": "^6.12.0",
    "auth0": "^2.36.1",
    "chart.js": "^3.5.1",
    "chartjs-adapter-date-fns": "^2.0.0",
    "cookies": "^0.8.0",
    "cross-fetch": "^3.1.4",
    "date-fns": "^2.23.0",
    "frappe-gantt": "^0.5.0",
    "fusioncharts": "^3.17.0",
    "http-proxy-middleware": "^2.0.1",
    "intro.js": "4.1.0",
    "intro.js-react": "^0.5.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "material-ui-popup-state": "^1.9.3",
    "multiparty": "^4.2.2",
    "next": "^11.1.2",
    "next-transpile-modules": "^8.0.0",
    "next-with-polyfill": "^0.1.5",
    "nextjs-basic-auth-middleware": "^0.2.1",
    "notistack": "^1.0.10",
    "patch-package": "^6.4.7",
    "path-parser": "^6.1.0",
    "postinstall-postinstall": "^2.1.0",
    "query-string": "^7.0.1",
    "react": "17.0.2",
    "react-chartjs-2": "^3.0.5",
    "react-dom": "17.0.2",
    "react-dropzone": "^11.4.0",
    "react-fusioncharts": "^3.1.2",
    "react-hook-form": "^7.15.3",
    "react-player": "^2.9.0",
    "rooks": "^5.7.1",
    "swr": "^0.5.6",
    "timeshift-js": "^1.1.1",
    "ts-node": "^10.2.1",
