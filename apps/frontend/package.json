    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "eslint-plugin-unused-imports": "^1.1.4",
    "jest": "^27.2.0",
    "nodemon": "^2.0.12",
    "npm-check-updates": "^11.8.5",
    "postcss": "^8.3.6",
    "prettier": "^2.4.1",
    "require-json5": "^1.1.0",
    "storybook-addon-material-ui": "^0.9.0-alpha.24",
    "ts-toolbelt": "^9.6.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  }
}
