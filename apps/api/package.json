{
    "name": "@ets-skel/api",
    "version": "0.0.1",
    "main": "./dist/index.js",
    "type": "commonjs",
    "license": "MIT",
    "private": true,
    "scripts": {
        "circular": "madge --circular --ts-config ./tsconfig.json --extensions ts -x dist/ ./",
        "clean": "rm -rf dist",
        "build": "sh ../../scripts/clean-dist.sh && NODE_OPTIONS=--max-old-space-size=4096 tsc --build",
        "watch": "yarn build --watch",
        "watch2": "tsc -w",
        "compile": "tsc --noEmit",
        "lint": "eslint .",
        "prettier": "prettier \"**/*.ts\"",
        "autofix": "yarn lint --fix && yarn prettier --write",
        "test": "jest",
        "testsuite": "yarn lint && yarn test && yarn circular",
        "debug": "node --inspect -r ./bootstrap.cjs ./start-server.ts",
        "debug:traced": "node -r ts-node/register --inspect ./start-server.ts",
        "dev": "NODE_ENV=development nodemon --exec yarn dev:polyfill",
        "dev:traced": "NODE_ENV=development nodemon --exec yarn debug:traced",
        "dev:compiled": "yarn build && concurrently -k -p \"[{name}]\" -n \"Build,Runtime\" -c \"cyan.bold,green.bold,blue.bold\" \"yarn watch\" \"yarn start:compiled\"",
        "dev:polyfill": "node --inspect -r ./bootstrap.cjs ./start-server.ts",
        "start": "yarn start:compiled",
        "start:compiled": "node --experimental-specifier-resolution=node -r source-map-support/register -r @effect-ts/system/Tracing/Enable ./dist/start-server.js",
        "start:polyfill": "node -r ./bootstrap.cjs ./start-server.ts",
        "ncu": "ncu",
        "update-libs": "yarn ncu -u --filter \"@ets-skel/*\" --pre 1"
    },
    "resolutions": {
        "typescript": "^4.4.3"
    },
    "dependencies": {
        "@ets-skel/client": "*",
        "@ets-skel/types": "*",
        "@azure/cosmos": "^3.14.1",
        "@azure/service-bus": "^7.3.0",
        "@azure/storage-blob": "^12.8.0",
        "@effect-ts-app/core": "^0.39.0",
        "@effect-ts-app/fluent": "^0.39.1",
        "@effect-ts-app/infra": "^0.39.1",
        "@effect-ts/core": "^0.44.4",
        "@effect-ts/express": "^0.20.3",
        "@effect-ts/fluent": "^0.3.12",
        "@effect-ts/fluent-node": "^0.1.2",
        "@effect-ts/monocle": "^0.35.0",
        "@effect-ts/morphic": "^0.37.1",
        "@effect-ts/node": "^0.29.0",
        "@sendgrid/mail": "^7.4.6",
        "@sentry/node": "^6.12.0",
        "auth0": "^2.36.1",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "date-fns": "^2.23.0",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-jwt": "^6.1.0",
        "express-jwt-authz": "^2.4.1",
        "faker": "^5.5.3",
        "jest-esm-transformer": "^1.0.0",
        "jwks-rsa": "^2.0.4",
        "jwt-decode": "^3.1.2",
        "lodash": "^4.17.21",
        "redis": "^3.1.2",
        "redlock": "^4.2.0",
        "redoc": "^2.0.0-rc.56",
        "redoc-express": "^1.0.0",
        "source-map-support": "^0.5.20",
        "swagger-ui-express": "^4.1.6"
    },
    "devDependencies": {
        "@jest/globals": "^27.2.0",
        "@types/auth0": "^2.33.5",
        "@types/body-parser": "^1.19.1",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/redis": "^2.8.32",
        "@types/swagger-ui-express": "^4.1.3",
        "require-json5": "^1.1.0"
    }
}
